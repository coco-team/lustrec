node mumuse (in1 : int^2^2; mult : int) returns (out : int^4)
var
	tmp : int^2^2;
	tmp2 : int^2;
let
	tmp = [ [ in1[0][0]*mult , in1[0][1] ] , [ in1[1][0]*mult , in1[1][1] ] ];
	tmp2 = add_vect_3 ( tmp[0] , [ mult,mult ] );
	out = [ tmp[0][0] , min_vect_2 (tmp2) , tmp[0][1] , tmp2[1] ];
tel
